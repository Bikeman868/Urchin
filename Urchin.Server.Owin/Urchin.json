{
    "urchin": {
        "server": {
            "persister": {
                "filePath": "c:\\temp\\rules.txt",
                "repository": "urchin"
            },
            "ui": {
                "dart": {
                    "uiRootUrl": "/ui",
                    "defaultDocument": "index.html",
                    "documentationRootUrl": "/ops/ui",
                    "dartUiRoot": "/ui/web",
                    "compiledUiRootUrl": "/ui/build/web"
                },
                "less": {
                    "rootUrl": "/ui",
                    "rootDirectory": "~\\ui",
                    "documentationRootUrl": "/ops/less"
                },
                "staticFiles": {
                    "staticFilesRootUrl": "/ui",
                    "rootDirectory": "~\\ui",
                    "documentationRootUrl": "/ops/staticFiles",
                    "maximumFileSizeToCache": 50000,
                    "maximumCacheTime": "01:00:00",
                    "fileExtensions": [
                        {
                            "extension": ".gif",
                            "mimeType": "image/gif"
                        },
                        {
                            "extension": ".ico",
                            "mimeType": "image/ico"
                        },
                        {
                            "extension": ".html",
                            "mimeType": "text/html"
                        },
                        {
                            "extension": ".css",
                            "mimeType": "text/css"
                        },
                        {
                            "extension":".js",
                            "mimeType":"application/javascript"
                        },
                        {
                            "extension":".dart",
                            "mimeType":"application/dart"
                        }
                    ]
                },
                "versioning": {
                    "version": 1,
                    "mimeTypes": ["text/html", "text/css", "application/javascript", "application/dart"],
                    "fileExtensions": [".css", ".html", ".ico", ".gif", ".js", ".dart"]
                },
                "outputCache": {
                    "maximumCacheTime": "00:01:00"
                }
            },
            "logon": {
                "administratorPassword": "password",
                "sessionExpiry": "00:45:00",
                "cookieName": "session_token"
            },
            "notFound": {
            },
            "exceptionReporter": {
                "localhost": true,
                "emailAddress": "bitbucket@mailinator.com",
                "subject": "Urchin unhandled exception"
            },
            "endpointDocumenter": {
                "path": "/ops/endpoints",
                "enabled": true,
                "requiredPermission": ""
            },
            "analysisReporter": {
                "path": "/ops/analytics",
                "enabled": true,
                "requiredPermission": "",
                "defaultFormat": "text/html"
            },
            "visualizer": {
                "path": "/ops/pipeline",
                "enabled": true,
                "requiredPermission": ""
            },
        }
    },

    "prius": {
        "databases": [
            {
                "name": "urchin",
                "type": "MySql",
                "connectionString": "Server=localhost;Port=3306;Uid=urchin;Pwd=password;Database=Urchin",
                "enabled": true
            }
        ],
        "fallbackPolicies": [
            { "name": "dontFailover", "allowedFailurePercent": 100 }
        ],
        "repositories": [
            {
                "name": "urchin",
                "clusters": [ { "databases": [ "urchin" ], "fallbackPolicy": "dontFailover" } ]
            }
        ]
    }
}